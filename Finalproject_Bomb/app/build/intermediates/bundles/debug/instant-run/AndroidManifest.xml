<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.androidtown.myapplication123"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label=""
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoTitleBar" >
        <activity
            android:name="org.androidtown.myapplication123.MainActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <!--
              <data
              android:scheme="@string/kakao_scheme"
                    android:host="@string/kakaolink_host" />
                -->
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />

        <activity
            android:name="org.androidtown.myapplication123.manual"
            android:label="@string/app_name"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="org.androidtown.myapplication123.hwList"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="org.androidtown.myapplication123.hwTT"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="org.androidtown.myapplication123.hwTtPlus"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="org.androidtown.myapplication123.hwCal"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="org.androidtown.myapplication123.hwPlus"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="org.androidtown.myapplication123.SplashActivity" />
        <activity android:name="org.androidtown.myapplication123.Splash2Activity" />
        <activity android:name="org.androidtown.myapplication123.Splash3Activity" />
        <activity android:name="org.androidtown.myapplication123.Splash4Activity" />

        <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

        <receiver
            android:name="org.androidtown.myapplication123.AlarmReceiver"
            android:enabled="true"
            android:exported="true"
            android:process=":remote" />

        <uses-permission android:name="android.permission.VIBRATE" />

        <receiver
            android:name="org.androidtown.myapplication123.BootReceiver"
            android:enabled="true"
            android:exported="false" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
    </application>

</manifest>